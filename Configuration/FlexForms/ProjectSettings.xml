<T3DataStructure>
    <ROOT>
        <sheetTitle>Filter</sheetTitle>
        <type>array</type>
        <el>
            <settings.hide_completed_projects>
                <label>LLL:EXT:academic_projects/Resources/Private/Language/locallang_be.xlf:flexform.filter.options.hide_completed_projects</label>
                <config>
                    <type>check</type>
                    <renderType>checkboxToggle</renderType>
                    <default>0</default>
                    <items>
                        <numIndex index="0">
                            <label></label>
                            <value>1</value>
                            <invertStateDisplay>1</invertStateDisplay>
                        </numIndex>
                    </items>
                </config>
            </settings.hide_completed_projects>
            <settings.filter.options>
                <label>LLL:EXT:academic_projects/Resources/Private/Language/locallang_be.xlf:flexform.filter.options.title</label>
                <config>
                    <type>check</type>
                    <renderType>checkboxToggle</renderType>
                    <default>0</default>
                    <items>
                        <numIndex index="0">
                            <label></label>
                            <value>1</value>
                            <invertStateDisplay>1</invertStateDisplay>
                        </numIndex>
                    </items>
                </config>
            </settings.filter.options>
            <settings.sorting.options>
                <label>LLL:EXT:academic_projects/Resources/Private/Language/locallang_be.xlf:flexform.sorting.options.title</label>
                <config>
                    <type>check</type>
                    <renderType>checkboxToggle</renderType>
                    <default>0</default>
                    <items>
                        <numIndex index="0">
                            <label></label>
                            <value>1</value>
                            <invertStateDisplay>1</invertStateDisplay>
                        </numIndex>
                    </items>
                </config>
            </settings.sorting.options>
            <settings.sorting>
                <label>LLL:EXT:academic_projects/Resources/Private/Language/locallang_be.xlf:flexform.sorting</label>
                <config>
                    <type>select</type>
                    <renderType>selectSingle</renderType>
                    <itemsProcFunc>FGTCLB\AcademicProjects\Backend\FormEngine\SortingItemsProcFunc->itemsProcFunc</itemsProcFunc>
                </config>
            </settings.sorting>
            <settings.categories>
                <label>LLL:EXT:core/Resources/Private/Language/locallang_tca.xlf:sys_category.categories</label>
                <description>LLL:EXT:academic_projects/Resources/Private/Language/locallang_be.xlf:flexform.category.description</description>
                <config>
                    <type>category</type>
                    <default>0</default>
                    <relationship>oneToMany</relationship>
                    <foreign_table>sys_category</foreign_table>
                    <foreign_table_where>AND {#sys_category}.{#type} != '' AND {#sys_category}.{#sys_language_uid} IN (-1, 0)</foreign_table_where>
                    <maxitems>9999</maxitems>
                    <MM>sys_category_record_mm</MM>
                    <MM_match_fields>
                        <fieldname>pi_flexform</fieldname>
                        <tablenames>tt_content</tablenames>
                    </MM_match_fields>
                    <MM_opposite_field>items</MM_opposite_field>
                </config>
            </settings.categories>
        </el>
    </ROOT>
</T3DataStructure>
